{
  "name": "stock-data.js",
  "version": "1.0.0",
  "description": "Easy to use stock market API for Node.js using WorldTradingData as source.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "pretest": "npm run lint",
    "test": "nyc mocha --require @babel/register --require @babel/polyfill --recursive --timeout 5000",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "nyc mocha --require @babel/register --require @babel/polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidsoederberg/stock-data.js.git"
  },
  "keywords": [
    "stock",
    "stocks",
    "data",
    "real time",
    "realtime",
    "historical",
    "market",
    "worldtradingdata",
    "api"
  ],
  "author": "David SÃ¶derberg <soederbergdavid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidsoederberg/stock-data/issues"
  },
  "homepage": "https://github.com/davidsoederberg/stock-data",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-minify": "^0.3.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^5.0.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2"
  }
}
